{
  "name": "@uniweb/jsonld-gen",
  "version": "0.1.0",
  "description": "Generate JSON-LD schemas and Open Graph meta tags for SEO",
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./react": "./dist/react/index.js",
    "./presets": "./dist/presets/index.js",
    "./presets/university": "./dist/presets/university.js",
    "./presets/video-library": "./dist/presets/video-library.js",
    "./presets/blog": "./dist/presets/blog.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "json-ld",
    "schema.org",
    "seo",
    "open-graph",
    "meta-tags",
    "structured-data",
    "metadata"
  ],
  "author": "Uniweb",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "rollup": "^4.9.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/uniweb/jsonld-gen"
  },
  "bugs": {
    "url": "https://github.com/uniweb/jsonld-gen/issues"
  }
}
